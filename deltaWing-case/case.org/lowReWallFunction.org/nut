FoamFile
{
    version     3.0;
    format      ascii;
    class       volScalarField;
    location    "0.org";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    lowerWall
    {
        type            nutLowReWallFunction;
        value           $internalField;
    }
    upperWall
    {
        type            calculated;
        value           uniform 0;
    }
    deltaWing
    {
        type            nutLowReWallFunction;
        value           uniform 0;
    }
    front
    {
        type            nutLowReWallFunction;
        value           $internalField;
    }

    back
    {
        type            symmetryPlane;
    }
}


