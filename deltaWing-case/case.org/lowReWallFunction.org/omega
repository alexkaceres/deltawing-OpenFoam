FoamFile
{
    version     2.1;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2.39;

wall omegaWallFunction;

boundaryField
{

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
   //   type            zeroGradient;
	type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    lowerWall
    {
      type            zeroGradient;

    }
    upperWall
    {
        type            slip;
    }
    deltaWing
    {
        type            zeroGradient;

    }
    front
    {
    	type            zeroGradient;

    }

    back
    {
        type            symmetryPlane;
    }
}


