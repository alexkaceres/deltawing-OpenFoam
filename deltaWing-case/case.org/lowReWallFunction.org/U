FoamFile
{
    version     2.1;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U.org;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField    uniform (1 0 0);

boundaryField
{

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
//      type            zeroGradient;
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    upperWall
    {
        type            slip;
    }
    deltaWing
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    front
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    back
    {
        type            symmetryPlane;
    }
}


